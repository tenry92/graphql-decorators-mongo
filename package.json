{
  "name": "graphql-decorators-mongo",
  "version": "0.1.0",
  "description": "Provide mongo access using annotated classes and the GraphQL query language.",
  "keywords": [
    "graphql",
    "decorators",
    "mongo"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "test": "mocha dist/test",
    "test:coverage": "nyc npm run test"
  },
  "author": "Simon \"Tenry\" Burchert",
  "repository": {
    "type": "git",
    "url": "https://github.com/tenry92/graphql-decorators-mongo.git"
  },
  "license": "MIT",
  "dependencies": {
    "change-case": "^3.0.2",
    "pluralize": "^7.0.0",
    "reflect-metadata": "^0.1.12"
  },
  "devDependencies": {
    "@tenry/graphql-decorators": "^0.1.5",
    "@types/graphql": "^0.12.5",
    "@types/mocha": "^2.2.48",
    "@types/mongodb": "^3.0.8",
    "chai": "^4.1.2",
    "graphql": "^0.13.2",
    "graphql-type-json": "^0.2.0",
    "mocha": "^5.0.4",
    "mongodb": "^3.0.4",
    "nyc": "^11.6.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "files": [
    "dist/index.js",
    "dist/decorators.js",
    "dist/manager.js",
    "dist/types.js",
    "dist/index.d.ts",
    "dist/decorators.d.ts",
    "dist/manager.d.ts",
    "dist/types.d.ts"
  ],
  "types": "./dist/index.d.ts",
  "nyc": {
    "include": [
      "dist"
    ],
    "exclude": [
      "dist/test"
    ]
  }
